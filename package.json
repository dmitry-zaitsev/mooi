{
  "name": "mooi-cli",
  "version": "1.0.15",
  "description": "Translation application for software projects",
  "author": "Dmitry Zaytsev",
  "bin": {
    "mooi": "./bin/run"
  },
  "homepage": "https://github.com/dmitry-zaitsev/mooi",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "repository": "dmitry-zaitsev/mooi",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "@sniptt/monads": "^0.5.10",
    "dirty-json": "^0.9.2",
    "fs-extra": "^11.1.1",
    "handlebars": "^4.7.7",
    "js-yaml": "^4.1.0",
    "openai": "^3.3.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@oclif/test": "^3.0.0",
    "@types/chai": "^4",
    "@types/fs-extra": "^11.0.1",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/parser": "^5.60.0",
    "chai": "^4",
    "eslint": "^8.41.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "jest": "^29.5.0",
    "mocha": "^10",
    "oclif": "^3",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0"
  },
  "oclif": {
    "bin": "mooi",
    "dirname": "mooi",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": "https://github.com/dmitry-zaitsev/mooi/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
